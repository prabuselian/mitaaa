                                    // decode &amp; and \'
                                    if (jQuery('.namatamu').length >= 1) {
                                        let tmpHtmlNamatamu = document.querySelectorAll('.namatamu')
                                        tmpHtmlNamatamu.forEach( (tamuTexts) => {
                                            tamuTexts.innerHTML = cleanIts(tamuTexts.innerHTML);
